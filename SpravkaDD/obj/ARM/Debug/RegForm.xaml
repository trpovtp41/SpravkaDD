<Page
    x:Class="SpravkaDD.RegForm"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:SpravkaDD"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    Background="{ThemeResource ApplicationPageBackgroundThemeBrush}">

    <Grid>
        <TextBlock x:Name="textBlock" Margin="80,47,79,0" TextWrapping="Wrap" Text="Пользователь" VerticalAlignment="Top" FontSize="36" TextAlignment="Center"/>
        <TextBox x:Name="imya" Margin="39,158,39,0" TextWrapping="Wrap" VerticalAlignment="Top" PlaceholderText="Введите имя" Height="17" RenderTransformOrigin="0.5,0.5"/>
        <TextBox x:Name="familia" Margin="39,254,39,0" TextWrapping="Wrap" VerticalAlignment="Top" PlaceholderText="Введите фамилию" Height="30"/>
        <TextBlock x:Name="textBlock1" HorizontalAlignment="Left" Margin="39,134,0,0" TextWrapping="Wrap" Text="Ваше имя" VerticalAlignment="Top" FontSize="20"/>
        <TextBlock x:Name="textBlock1_Copy" HorizontalAlignment="Left" Margin="80,225,0,0" TextWrapping="Wrap" VerticalAlignment="Top" FontSize="20"/>
        <TextBlock x:Name="textBlock1_Copy1" HorizontalAlignment="Left" Margin="39,225,0,0" TextWrapping="Wrap" Text="Ваша фамилия" VerticalAlignment="Top" FontSize="20"/>
        <RadioButton x:Name="Male" Content="Мужской" HorizontalAlignment="Left" Margin="39,325,0,0" VerticalAlignment="Top"/>
        <RadioButton x:Name="Female" Content="Женский" HorizontalAlignment="Left" Margin="39,386,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.551,0.71"/>
        <TextBlock x:Name="textBlock1_Copy2" HorizontalAlignment="Left" Margin="39,312,0,0" TextWrapping="Wrap" Text="Ваш пол" VerticalAlignment="Top" FontSize="20"/>
        <TextBox x:Name="familia_Copy" Margin="39,491,39,0" TextWrapping="Wrap" VerticalAlignment="Top" PlaceholderText="ДД.ММ.ГГГГ" Height="30"/>
        <TextBlock x:Name="textBlock1_Copy3" HorizontalAlignment="Left" Margin="39,462,0,0" TextWrapping="Wrap" Text="Дата рождения" VerticalAlignment="Top" FontSize="20"/>
        <Button x:ConnectionId='1' x:Name="button" Content="Далее..." HorizontalAlignment="Left" Margin="215,559,0,0" VerticalAlignment="Top" Width="146"                     />
        <Button x:ConnectionId='2' x:Name="button1" Content="Назад" HorizontalAlignment="Left" Margin="39,559,0,0" VerticalAlignment="Top"                      />

    </Grid>
</Page>

